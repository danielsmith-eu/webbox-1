  <!-- example of an annotation that anchored to an _entity reference_ that might occur on _any_ page -->
  <rdf:Description rdf:about="http://hip.cat/lightsaber#emax-239892891239823982932-233">
    <rdf:type rdf:resource="http://hip.cat/lightsaber#EntityReferenceAnnotation"/>
    <ls:src_url>http://web.mit.edu</ls:src_url>    
    <ls:src_reference>Massachusetts Institute of Technology</ls:src_reference>
    <ls:referrent rdf:resource="http://web.mit.edu/about#"/>
    <ls:contents>
            <rdf:Seq>
              <rdf:li>
                <rdf:Description rdf:about="http://hip.cat/lightsaber#emax-239892891239823982932-233-tc">
                  <rdf:type rdf:resource="http://hip.cat/lightsaber#TextualContent"/>
                  <ls:content>IHTFP!!!</ls:content>
                </rdf:Description>
              </rdf:li>
            </rdf:Seq>
    </ls:contents>
    <dc:author rdf:resource="http://hip.cat/emax/foaf.rdf#emax"/>
    <dc:creation_time rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2007-05-28T23:09:33.426Z</dc:creation_time>
    <dc:last_modified_time rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2007-05-28T23:09:33.426Z</dc:last_modified_time>    
  </rdf:Description>  
</rdf:RDF>
