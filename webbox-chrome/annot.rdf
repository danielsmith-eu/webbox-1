<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:dc="http://purl.org/dc/elements/1.1/"
    xmlns:rww="http://www.w3.org/2011/10/12-rww#"
    xmlns:ls="http://hip.cat/lightsaber#"
    xmlns:myfoaf="http://hip.cat/emax/foaf.rdf#"    
>
  <!-- example of an annotation that is anchored textually to a _page_ -->
  <rdf:Description rdf:about="http://hip.cat/lightsaber#emax-239892891239823982932-232">
    <rdf:type rdf:resource="http://hip.cat/lightsaber#TextualPageAnnotation"/>
    <ls:source_url>http://web.mit.edu</ls:source_url>
    <ls:anchorxpath>/html/body/h2</ls:anchorxpath>        
    <ls:contents>
            <rdf:Seq>
              <!-- I propose that an annotation could consist of a list of stuff both structured and unstructured --> 
              <rdf:li>
                <rdf:Description rdf:about="http://hip.cat/lightsaber#emax-239892891239823982932-232-tc">
                  <rdf:type rdf:resource="http://hip.cat/lightsaber#TextualContent"/>
                  <ls:content>I love RDF.</ls:content>
                </rdf:Description>
              </rdf:li>
            </rdf:Seq>
    </ls:contents>
    <dc:author rdf:resource="http://hip.cat/emax/foaf.rdf#emax"/>
    <dc:creation_time rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2007-05-28T23:09:33.426Z</dc:creation_time>
    <dc:last_modified_time rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2007-05-28T23:09:33.426Z</dc:last_modified_time>
    <!--
